#Descriptive Analysis 
```{r}
#WHO Data Set 
covid <- read_csv("https://covid19.who.int/WHO-COVID-19-global-data.csv")
names(covid) 

#Merged Data of Policy and CFR calculation 
final<- read.csv("C:/Users/eshna/Downloads/final_data.csv")

#Nigeria
nigeria = final %>% filter(.,Country == "Nigeria")
nigeria= mutate(nigeria, time = seq(1:184), CFR= nigeria$Cumulative_deaths/nigeria$Cumulative_cases)

plot(nigeria$time, nigeria$New_cases, main="Plot of New Cases over time for Nigeria")
#
plot(nigeria$time, nigeria$New_deaths, main="Plot of New Deaths over time for Nigeria")
#
plot(nigeria$time, nigeria$Cumulative_cases, main="Plot of Cumulative Cases over time for Nigeria")
#
plot(nigeria$time, nigeria$Cumulative_deaths, main="Plot of Cumulative Deaths over time for Nigeria")


#Ethiopia 
ethiopia = final %>% filter(.,Country == "Ethiopia" & Date >= "2020-04-19" & Date <= "2020-07-27" )
ethiopia= mutate(ethiopia, time = seq(1:100), CFR= ethiopia$Cumulative_deaths/ethiopia$Cumulative_cases)

plot(ethiopia$time, ethiopia$New_cases, main="Plot of New Cases over time for Ethiopia")
#
plot(ethiopia$time, ethiopia$New_deaths, main="Plot of New Deaths over time for Ethiopia")
#
plot(ethiopia$time, ethiopia$Cumulative_cases, main="Plot of Cumulative Cases over time for Ethiopia")
#
plot(ethiopia$time, ethiopia$Cumulative_deaths, main="Plot of Cumulative Deaths over time for Ethiopia")
#


#US
usa = final %>% filter(.,Country == "United States" & Date >= "2020-03-03" & Date <= "2020-06-10" )
usa= mutate(usa, time = seq(1:100), CFR= usa$Cumulative_deaths/usa$Cumulative_cases)

plot(usa$time, usa$New_cases, main="Plot of New Cases over time for USA")
#
plot(usa$time, usa$New_deaths, main="Plot of New Deaths over time for USA")
#
plot(usa$time, usa$Cumulative_cases, main="Plot of Cumulative Cases over time for USA")
#
plot(usa$time, usa$Cumulative_deaths, main="Plot of Cumulative Deaths over time for USA")
#


#Brazil
brazil = final %>% filter(.,Country == "Brazil" & Date >= "2020-03-13" & Date <= "2020-06-20" )
brazil= mutate(brazil, time = seq(1:100), CFR= brazil$Cumulative_deaths/brazil$Cumulative_cases)

plot(brazil$time, brazil$New_cases, main="Plot of New Cases over time for Brazil")
#
plot(brazil$time, brazil$New_deaths, main="Plot of New Deaths over time for Brazil")
#
plot(brazil$time, brazil$Cumulative_cases, main="Plot of Cumulative Cases over time for Brazil")
#
plot(brazil$time, brazil$Cumulative_deaths, main="Plot of Cumulative Deaths over time for Brazil")
#



#India
india = final %>% filter(.,Country == "India" & Date >= "2020-03-15" & Date <= "2020-06-22" )
india= mutate(india, time = seq(1:100), CFR= india$Cumulative_deaths/india$Cumulative_cases)

plot(india$time, india$New_cases, main="Plot of New Cases over time for India")
#
plot(india$time, india$New_deaths, main="Plot of New Deaths over time for India")
#
plot(india$time, india$Cumulative_cases, main="Plot of Cumulative Cases over time for India")
#
plot(india$time, india$Cumulative_deaths, main="Plot of Cumulative Deaths over time for India")
#

#Indonesia
indonesia = final %>% filter(.,Country == "Indonesia" & Date >= "2020-03-15" & Date <= "2020-06-22" )
indonesia= mutate(indonesia, time = seq(1:100), CFR= indonesia$Cumulative_deaths/indonesia$Cumulative_cases)

plot(indonesia$time, indonesia$New_cases, main="Plot of New Cases over time for Indonesia")
#
plot(indonesia$time, indonesia$New_deaths, main="Plot of New Deaths over time for Indonesia")
#
plot(indonesia$time, indonesia$Cumulative_cases, main="Plot of Cumulative Cases over time for Indonesia")
#
plot(indonesia$time, indonesia$Cumulative_deaths, main="Plot of Cumulative Deaths over time for Indonesia")
#


#Russia
russia = final %>% filter(.,Country == "Russian Federation" & Date >= "2020-03-19" & Date <= "2020-06-26" )
russia = mutate(russia, time = seq(1:100))

plot(russia$time, russia$New_cases, main="Plot of New Cases over time for Russia")
#
plot(russia$time, russia$New_deaths, main="Plot of New Deaths over time for Russia")
#
plot(russia$time, russia$Cumulative_cases, main="Plot of Cumulative Cases over time for Russia")
#
plot(russia$time, russia$Cumulative_deaths, main="Plot of Cumulative Deaths over time for Russia")
#

#Turkey
turkey = final %>% filter(.,Country == "Turkey" & Date >= "2020-03-19" & Date <= "2020-06-26" )
turkey = mutate(turkey, Time = seq(1:100))

plot(turkey$time, turkey$New_cases, main="Plot of New Cases over time for Turkey")
#
plot(turkey$time, turkey$New_deaths, main="Plot of New Deaths over time for Turkey")
#
plot(turkey$time, turkey$Cumulative_cases, main="Plot of Cumulative Cases over time for Turkey")
#
plot(turkey$time, russia$Cumulative_deaths, main="Plot of Cumulative Deaths over time for Turkey")
#

#Pakistan
pakistan = final %>% filter(.,Country == "Pakistan" & Date >= "2020-03-17" & Date <= "2020-06-24" )
pakistan = mutate(pakistan, Time = seq(1:100))

plot(pakistan$time, pakistan$New_cases, main="Plot of New Cases over time for Pakistan")
#
plot(pakistan$time, pakistan$New_deaths, main="Plot of New Deaths over time for Pakistan")
#
plot(pakistan$time, pakistan$Cumulative_cases, main="Plot of Cumulative Cases over time for Pakistan")
#
plot(pakistan$time, pakistan$Cumulative_deaths, main="Plot of Cumulative Deaths over time for Pakistan")
#

#Egypt
egypt=filter(final,(Country=="Egypt"))
egypt=egypt[73:172,]
egypt=mutate(egypt,time=seq(1:100))

plot(egypt$time, egypt$New_cases, main="Plot of New Cases over time for Egypt")
#
plot(egypt$time, egypt$New_deaths, main="Plot of New Deaths over time for Egypt")
#
plot(egypt$time, egypt$Cumulative_cases, main="Plot of Cumulative Cases over time for Egypt")
#
plot(egypt$time, egypt$Cumulative_deaths, main="Plot of Cumulative Deaths over time for Egypt")
#


#China 
china=filter(final,(Country=="China"))
china=china[17:116,]
china=mutate(china,time=seq(1:100))

plot(china$time, china$New_cases, main="Plot of New Cases over time for China")
#
plot(china$time, china$New_deaths, main="Plot of New Deaths over time for China")
#
plot(china$time, china$Cumulative_cases, main="Plot of Cumulative Cases over time for China")
#
plot(china$time, china$Cumulative_deaths, main="Plot of Cumulative Deaths over time for China")
#

#Japan
japan=filter(final,(Country=="Japan"))
japan=japan[50:149,]
japan=mutate(japan,time=seq(1:100))

plot(japan$time, japan$New_cases, main="Plot of New Cases over time for Japan")
#
plot(japan$time, japan$New_deaths, main="Plot of New Deaths over time for Japan")
#
plot(japan$time, japan$Cumulative_cases, main="Plot of Cumulative Cases over time for Japan")
#
plot(japan$time, japan$Cumulative_deaths, main="Plot of Cumulative Deaths over time for Japan")
#



